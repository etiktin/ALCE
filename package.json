{
  "name": "alce",
  "version": "1.0.1",
  "description": "Accepting Language Config Environment",
  "keywords": [
    "json parser whitespace comments configuration"
  ],
  "dependencies": {
    "esprima": "1.2.x",
    "estraverse": "1.5.x"
  },
  "devDependencies": {
    "grunt": "~0.4.1",
    "grunt-mocha-cov": "0.x.x",
    "mocha": "1.x.x",
    "should": "3.x.x"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/walmartlabs/alce.git"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "main": "lib/alce.js",
  "scripts": {
    "blanket": {
      "pattern": "//^((?!/node_modules/)(?!/test/).)*$/",
      "data-cover-flags": {
        "branchTracking": true
      }
    },
    "test": "./node_modules/.bin/mocha"
  }
}
